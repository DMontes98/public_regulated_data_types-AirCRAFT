# Sends specific 16-bit raw data point to another node

uint32 timestamp
# 32-bit timestamp in microseconds (Watch out! 1.2h overflow time)

uint16 data
# 16-bit raw data point

uint8 flags
# 8-bit flags

# Flags
uint8 DATA_SOURCE_1   = 0
uint8 DATA_SOURCE_2   = 1
uint8 DATA_SOURCE_3   = 2
uint8 DATA_SOURCE_4   = 3

@extent 56